     <mx:ComboBox
         id="imagesComboBox"
         labelField="title"
         dataProvider="{images}"
         focusEnabled="false"
         horizontalCenter="0"
         top="8"
         height="28"
         width="320"
         />

    <openzoom:MultiScaleImage
        id="image"
        top="{imagesComboBox.height + 16}"
        right="0"
        bottom="0"
        left="0"
        source="{imagesComboBox.selectedItem.dzi}"
        focusEnabled="false"
        tabEnabled="false"
        >
        <openzoom:transformer>
            <transformers:TweenerTransformer/>
        </openzoom:transformer>
        <openzoom:controllers>
            <controllers:MouseController/>
            <controllers:KeyboardController/>
        </openzoom:controllers>
    </openzoom:MultiScaleImage>

