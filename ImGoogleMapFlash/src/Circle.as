package {		import flash.display.*;		public class Circle extends Sprite {				public function Circle(name:String, cont:DisplayObjectContainer, x:int, y:int, radius:int, color:uint){			this.blendMode = BlendMode.DARKEN;			this.graphics.lineStyle(1, color, 1, false, LineScaleMode.NONE);			this.graphics.beginFill(color, 0.5);			this.graphics.drawCircle(0,0,radius);			this.graphics.endFill();			this.x = x;			this.y = y;			this.name = name;			cont.addChild(this);		}			}}